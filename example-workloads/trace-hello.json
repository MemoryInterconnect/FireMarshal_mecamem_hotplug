{
  "name" : "trace-hello",
  "base" : "br-base.json",
  "iceblk-persistent-modification" : true,
  "spike-args" : "--device=trace_encoder_ctrl --trace l ",
  "overlay" : "overlay",
  "host-init" : "host-init.sh",
  "jobs" : [
    {
      "name" : "run-chores",
      "iceblk-persistent-modification" : true,
      "outputs" : [ "/root/chores/driver_map.txt", "/root/chores/jump_label_patch_map.txt" ],
      "run" : "run-chores.sh"
    },
    {
      "name" : "trace-hello",
      "command" : "/root/trace-hello/trace-submit /root/trace-hello/hello"
    }
  ]
}